{
    "version": 2,
    "builds": [
      {
        "src": "api/resume.js",
        "use": "@vercel/node",
        "config": {
          "maxDuration": 60
        }
      },
      {
        "src": "api/receipt.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/resume",
        "dest": "/api/resume.js"
      },
      {
        "src": "/api/receipt",
        "dest": "/api/receipt.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }